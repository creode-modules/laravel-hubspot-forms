{
    "name": "creode/laravel-hubspot-forms",
    "description": "This package allows you to embed Hubspot forms in your laravel application using a Blade Component.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "hubspot/api-client": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Creode\\LaravelHubspotForms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creode\\LaravelHubspotForms\\LaravelHubspotFormsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
